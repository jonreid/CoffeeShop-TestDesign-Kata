{
    "lineWidth": 80,
    "indentWidth": 4,
    "useTabs": false,
    "newLineKind": "lf",
    "includes": ["**/*.md", "**/*.yml", "**/*.yaml", "../**/*.md", "../**/*.yml", "../**/*.yaml"],
    "excludes": ["node_modules/**/*"],
    "plugins": [
        "https://plugins.dprint.dev/markdown-0.18.0.wasm",
        "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.0.wasm"
    ]
}